package Examples;

public class EX2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int a[] = new int [5];
        a[0] = 1;
        a[1] = 10;
        a[2] = 100;
        for (int i =0; i < a.length; i++) 
        {
        	int value = a[i];
        	System.out.println(value);
        }
	}

}
