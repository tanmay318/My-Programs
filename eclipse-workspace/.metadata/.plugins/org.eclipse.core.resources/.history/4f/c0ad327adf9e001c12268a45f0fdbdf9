package MultiArray;

public class MultiplicationArray {
public static void main(String[] args) {
	int a[][] = {{1,2},{3,4}};
	int b[][] = {{2,3},{3,1}};
	int c[][] =new int [2][2];
	for (int i =0;i<a.length;i++) 
	{
		for(int j=0;j<a.length;j++) {
			
			System.out.print("\t"+a[i][j]);
		}System.out.println();
	}
	System.out.println();
	for(int i=0;i<a.length;i++) 
	{
		for(int j=0;j<a.length;j++) {
			
			System.out.print("\t"+b[i][j]);
		}
			
	}
}
}
